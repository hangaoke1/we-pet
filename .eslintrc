{
  "extends": ["taro"],
  "rules": {
    "no-unused-vars": ["error", { "varsIgnorePattern": "Taro" }],
    "react/jsx-filename-extension": [
      1,
      { "extensions": [".js", ".jsx", ".tsx"] }
    ],
    "jsx-quotes": 0,
    "react/sort-comp": 0,
    "no-shadow": 0,
    "import/prefer-default-export": 0
  },
  "parser": "babel-eslint"
}
